<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="physicians">
  <thead>
    <tr>
      <th>Email</th>
      <th>Type</th>
      <th>Admin</th>
      <th>Created At</th>
      <th>Last Edited At</th>
      <th>Login Count</th>
      <th>Last Sign In At</th>
      <th class="td-actions">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @physicians.each do |physician| %>
      <tr>
        <td><%= physician.email %></td>
        <td><%= physician.type %></td>
        <td><%= physician.admin %></td>
        <td><%= physician.created_at.to_s(:datetime).humanize %></td>
        <td><%= physician.updated_at.to_s(:datetime).humanize %></td>
        <td><%= physician.sign_in_count %></td>
        <td><%if physician.last_sign_in_at.blank? %>Never logged in<% else %><%= time_ago_in_words(physician.last_sign_in_at.to_datetime)+' ago' %><% end %></td>
        <td class="td-actions">
          <%= link_to "Edit", edit_physician_path(physician), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', physician, :data => { :confirm => 'Are you sure?' }, method: :delete, :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>